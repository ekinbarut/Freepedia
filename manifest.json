{
    "name": "Freepedia",
    "version": "0.1",
    "description": "Access wikipedia from turkey.",
    "manifest_version": 2,
    "author" : "Ekin Barut",
    "homepage_url" : "http://ekinbarut.com",
    "browser_action": {
        "default_icon" : "icon.png"
    },
    "icons" : {
        "16" : "icon.png",
        "48" : "icon.png",
        "128" : "icon.png"
    },
    "background" : {
        "scripts" : [
            "background.js"
        ],
        "persistent" : true
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://*.wikipedia.org/",
        "https://*.wikipedia.org/"
      ]
  }